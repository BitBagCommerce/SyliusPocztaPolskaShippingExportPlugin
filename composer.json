{
    "name": "bitbag/poczta-polska-shipping-export-plugin",
    "type": "sylius-plugin",
    "description": "Poczta polska shipping export plugin for Sylius based applications.",
    "license": "MIT",
    "authors": [
        {
            "name": "Damian Murawski",
            "email": "damian.murawski@bitbag.pl"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/damonsson/ElektronicznyNadawca"
        }
    ],
    "require": {
        "php": "^7.1",
        "sylius/sylius": "^1.1",
        "bitbag/shipping-export-plugin": "^1.2",
        "printu/elektroniczny-nadawca": "dev-master"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.2",
        "phpunit/phpunit": "^5.6",
        "behat/behat": "^3.3",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/context-service-extension": "^0.3",
        "friends-of-behat/cross-container-extension": "^0.2",
        "friends-of-behat/performance-extension": "^1.0",
        "friends-of-behat/service-container-extension": "^0.3",
        "friends-of-behat/symfony-extension": "^0.2.1",
        "friends-of-behat/variadic-extension": "^0.1",
        "lakion/mink-debug-extension": "^1.2.3",
        "se/selenium-server-standalone": "^2.52"
    },
    "autoload": {
        "psr-4": {
            "BitBag\\PocztaPolskaShippingExportPlugin\\": "src/",
            "Tests\\BitBag\\PocztaPolskaShippingExportPlugin\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
